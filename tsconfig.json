{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@login/*": ["loginUI/src/*"],
      "@main/*": ["mainServiceUI/src/*"],
      "@assets/*": ["loginUI/assets/*"],
      "@ui/*": ["loginUI/src/ui/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
